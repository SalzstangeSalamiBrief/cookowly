{
  "extends": [
    "next/core-web-vitals",
    "plugin:@next/next/recommended",
    "prettier",
    "airbnb",
    "airbnb/hooks",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "indent": ["error", 2],
    "react/jsx-indent": ["error", 2],
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    "react/jsx-one-expression-per-line": "off"
  }
}
